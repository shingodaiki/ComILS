				<Setter TargetName="textBox" Property="Visibility" Value="Visible" />
				<Setter TargetName="textBlock" Property="Visibility" Value="Hidden" />
			</Trigger>
			<Trigger Property="IsKeyboardFocusWithin" Value="True">
				<Setter TargetName="textBox" Property="Visibility" Value="Visible" />
				<Setter TargetName="textBlock" Property="Visibility" Value="Hidden" />
			</Trigger>
		</DataTemplate.Triggers>
	</DataTemplate>

